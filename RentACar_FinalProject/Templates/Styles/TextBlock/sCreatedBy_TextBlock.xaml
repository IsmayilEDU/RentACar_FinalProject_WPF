<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="sCreatedBy_TextBlock" TargetType="TextBlock">
        <Setter Property="Text" Value="Created By ISMAYIL KERIMZADE" />
        <Setter Property="Foreground" Value="{StaticResource cBackgroundOfTextBox}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="FontFamily" Value="{StaticResource fTextBlock}" />
        <Setter Property="FontSize" Value="10" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Center" />
    </Style>
</ResourceDictionary>